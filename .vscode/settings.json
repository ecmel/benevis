{
    "files.associations": {
        "*.toml": "ini"
    },
    "files.exclude": {
        "**/__init__.py": true,
        "**/__main__.py": true,
        "**/__pycache__": true,
        "**/.pytest_cache": true
    },
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "task.problemMatchers.neverPrompt": {
        "shell": true
    },
    "python.terminal.activateEnvInCurrentTerminal": true
}